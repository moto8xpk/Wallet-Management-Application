<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="vn.com.dtt.ungdung16doana.AfterLogOn.AdminUIActivity">

    <ListView
        android:id="@+id/lv_NguoiDung"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:layout_marginBottom="145dp"
        android:layout_marginLeft="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="RtlHardcoded"/>

    <Button
        android:id="@+id/btnViewListUser"
        android:layout_width="148dp"
        android:layout_height="45dp"
        android:layout_marginLeft="-16dp"
        android:layout_marginTop="0dp"
        android:text="@string/btnAdminView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lv_NguoiDung"
        tools:ignore="RtlHardcoded"
        />

    <Button
        android:id="@+id/btnAdminAddWalletType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="3dp"
        android:text="ADD WALLET TYPE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded"/>

    <Button
        android:id="@+id/btnAdminAddMoneyCurrent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD MONEY CURRENT"
        android:layout_marginRight="67dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="-16dp"
        tools:ignore="HardcodedText,RtlHardcoded"/>

    <Button
        android:id="@+id/btnAdminAddIncomeType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD INCOME TYPE"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/btnViewListUser"
        android:layout_marginLeft="3dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded"/>

    <Button
        android:id="@+id/btnAdminAddOutcomeType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD OUTCOME TYPE"
        android:layout_marginLeft="151dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/lv_NguoiDung"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        tools:ignore="HardcodedText,RtlHardcoded"/>
</android.support.constraint.ConstraintLayout>
